<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <Preference android:key="@string/pref_key_version">
    </Preference>
    <Preference
        android:key="@string/pref_key_github"
